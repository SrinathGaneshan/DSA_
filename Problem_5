import java.util.Scanner;
class Main 
{
    public static void main(String[] args) 
    {
        
       Scanner sc = new Scanner(System.in);
        
        System.out.println("Enter the value of x:");
        int x = sc.nextInt();
        
        System.out.println("Enter the value of y:");
        int y = sc.nextInt();
        
        for (int i = 0; i < y; i++) 
        {
            System.out.println(x);
        }
   }
}
